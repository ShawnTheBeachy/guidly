<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us"
              Name="GuidGeneratorEnUsCommandSet">

    <AppName>Guidly</AppName>
    <Example>New GUID</Example>

    <Command Name="generateGuid">
      <Example>Generate GUID</Example>
      <ListenFor RequireAppName="BeforeOrAfterPhrase">generate [new] guid</ListenFor>
      <Feedback>Generating a new GUID</Feedback>
      <VoiceCommandService Target="GuidlyCommandService" />
    </Command>

    <Command Name="newGuid">
      <Example>New GUID</Example>
      <ListenFor RequireAppName="BeforeOrAfterPhrase">[generate] new guid</ListenFor>
      <Feedback>Generating a new GUID</Feedback>
      <VoiceCommandService Target="GuidlyCommandService" />
    </Command>

  </CommandSet>
</VoiceCommands>
